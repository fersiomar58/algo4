PROCEDURE tritab(VAR a,b : INTEGER)
VAR
    aux :INTEGER;
BEGIN
    aux:=a;
    a:=b;
    b:=aux;
END
ALGORITHM omar;
VAR
    tab : ARRAY_OF INTEGER;
    i,j :INTEGER;
    n : INTEGER.
BEGIN
n:=tab.length-1
FOR i FROM 0 TO n STEP 1  DO
    j:=i;
    WHILE (j>0 AND tab[j]<tab[j-1]) DO
        tritab(tab[j],tab[j-1])
        j:=j-1;
    END_WHILE
END_FOR
    
END